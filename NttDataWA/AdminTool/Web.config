  <system.web>
    <httpModules>
      <!-- 
      <add name="SessionTimeout" type="DocsPAWA.gestione.sessione.SessionTimeout,DocsPaWA"/>-->
    </httpModules>
    <compilation debug="true" targetFramework="4.0">
      <assemblies>
        <add assembly="System.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A"/>
      </assemblies>
    </compilation>
    <authentication mode="Windows"/>
    <globalization culture="it-IT" requestEncoding="utf-8" responseEncoding="utf-8"/>
    <pages enableEventValidation="false" validateRequest="false" controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
    <httpRuntime executionTimeout="3600" maxRequestLength="100000"/>
    <!--  SESSION STATE SETTINGS
          By default ASP .NET uses cookies to identify which requests belong to a particular session. 
          If cookies are not available, a session can be tracked by adding a session identifier to the URL. 
          To disable cookies, set sessionState cookieless="true".-->
    <sessionState mode="InProc" cookieless="false" timeout="20"/>
  </system.web>
  <system.webServer>
    <security>
      <requestFiltering>
        <hiddenSegments>
          <remove segment="bin"/>
        </hiddenSegments>
        <requestLimits maxAllowedContentLength="90000000"/>
      </requestFiltering>
    </security>
  </system.webServer>
  <applicationSettings>
    <SAAdminTool.Properties.Settings>
      <setting name="DocsPaWA_Caching_CachingServices" serializeAs="String">
        <value>http://localhost/CachingWS/CachingServices.asmx</value>
      </setting>
      <!-- 
    http://test.pitre.tn.it/pre_collaudo/backend/docspaws.asmx
    http://localhost/Vt-Docs/DocsPaWS/DocsPaWS.asmx
    -->
      <setting name="DocsPaWA_DocsPaWR_DocsPaWebService" serializeAs="String">
        <value>http://localhost/Vt-Docs/DocsPaWS/DocsPaWS.asmx</value>
      </setting>
    </SAAdminTool.Properties.Settings>
  </applicationSettings>
  <log4net debug="true">
    <appender name="FE" type="DocsPaUtils.LogsManagement.DateTimeRollingFileAppender">
      <environmentHandler value="DocsPAWA.utils.DefaultEnvironmentHandler, DocsPaWA"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%property{userId}] %-5level %logger %M - %message%newline"/>
      </layout>
      <filter type="DocsPaUtils.LogsManagement.EnableFilter, DocsPaUtils">
        <environmentHandler value="DocsPAWA.utils.DefaultEnvironmentHandler, DocsPaWA"/>
      </filter>
    </appender>
    <root>
      <level value="DEBUG"/>
    </root>
    <logger name="DocsPAWA">
      <appender-ref ref="FE"/>
    </logger>
  </log4net>
</configuration>
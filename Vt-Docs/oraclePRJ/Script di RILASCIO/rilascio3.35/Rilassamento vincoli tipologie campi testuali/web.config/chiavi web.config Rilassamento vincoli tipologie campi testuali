    <!--chiave di web.config di Frontend per il numero massimo di caratteri per tipologia fascicolo su campo di testo-->
    <add key="NUM_CHAR_CAMPO_TESTO_MAX_FASC" value="255"/>
    <!--chiave di web.config di Frontend per il numero minimo di caratteri per tipologia fascicolo su campo di testo-->
    <add key="NUM_CHAR_CAMPO_TESTO_MIN_FASC" value="2"/>
    <!--chiave di web.config di Frontend per il numero massimo di caratteri per tipologia documento su campo di testo-->
    <add key="NUM_CHAR_CAMPO_TESTO_MAX_DOC" value="255"/>
    <!--chiave di web.config di Frontend per il numero minimo di caratteri per tipologia documento su campo di testo-->
    <add key="NUM_CHAR_CAMPO_TESTO_MIN_DOC" value="2"/>